<Window x:Class="_11111.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_11111"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <hc:DatePicker x:Name="myDT" Grid.Row="0" Width="180" Height="80"></hc:DatePicker>
      
        
        <TextBox x:Name="b" Grid.Row="1" Height="100" Width="230"></TextBox>
        <Button Grid.Row="3" Content="确定" Click="Button_Click"></Button>
    </Grid> 
</Window>
